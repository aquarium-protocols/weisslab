argument
  debug_mode: string, "Yes or No"
  group: group, "choose the group to assign this metacol"
end

place tasks_inputs
  protocol: "aqualib:workflows/general/tasks_inputs.rb"
  group: group
  argument
    debug_mode: debug_mode
    group: group
    task_name: "CHO Cytometry"
  end
  marked: true
  start: now()
  window: hours(1)
end

place transfect
  protocol: "aqualib:workflows/mammalian/CHO_cytometry.rb"
  group: group
  marked: false
  start: now()
  window: hours(1)
end

wire tasks_inputs => trypsinized

transition [ tasks_inputs ] => [ trypsinized ] when completed(0) end
transition [ trypsinized ] => [] when !error(0) && completed(0) end

transition [ trypsinized ] => [ trypsinized ] when error(0) end
